{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "builds": [
        {
            "src": "api/flask_app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/flask_app.py"
        }
    ],
    "functions": {
        "api/flask_app.py": {
            "maxDuration": 30
        }
    }
}